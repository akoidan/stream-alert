{
  "name": "stream-alert",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:debug": "node  --inspect -r tsconfig-paths/register -r ts-node/register src/main.ts",
    "types": "node-config-ts",
    "transpile": "nest build",
    "pkg:win32": "nexe dist/main.js --build --targets windows-x64-20.11.1 --resource node_modules/thread-stream/lib/worker.js --resource build/Debug/server.node",
    "build": "cmake-js build --debug"
  },
  "engines": {
    "node": ">=20.0.0 <21",
    "npm": "please-use-yarn",
    "yarn": ">= 1.17.3"
  },
  "nexe": {
    "input": "dist/main.js",
    "resources": [
      "node_modules/thread-stream/lib/worker.js",
      "build/Release/server.node",
      "build/Debug/server.node"
    ]
  },
  "binary": {
    "napi_versions": [
      8
    ]
  },
  "bin": "dist/main.js",
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.8",
    "@types/bindings": "^1.5.5",
    "@types/cli-color": "^2.0.6",
    "@types/node": "^24.10.0",
    "cmake-js": "^7.1.0",
    "node-addon-api": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "nexe": "5.0.0-beta.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "bindings": "^1.5.0",
    "cli-color": "^2.0.4",
    "node-config-ts": "^3.3.1",
    "node-ts-config": "^1.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "7.1.0",
    "telegraf": "^4.16.3"
  }
}
