{
  "name": "stream-alert",
  "version": "1.0.1",
  "description": "",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "schema": "zod2md --entry ./src/config/config-zod-schema.ts --title Config --output CONFIG.md",
    "debug": "node  --inspect -r tsconfig-paths/register -r ts-node/register src/main.ts",
    "cmake": "cmake-js build",
    "cmake:debug": "cmake-js build --debug",
    "nest": "nest build",
    "lint": "eslint --ext .ts --max-warnings=0 src",
    "autoformat": "eslint --ext .ts --max-warnings=0 --fix src",
    "build": "npm-run-all -s cmake nest esbuild native",
    "esbuild": "node esbuild.config.js",
    "native:prep": "node --experimental-sea-config sea-config.json",
    "native:copynode": "node -e \"require('fs').copyFileSync(process.execPath, 'dist/stream-alert.exe')\"",
    "native:postject": "postject dist/stream-alert.exe NODE_SEA_BLOB dist/sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2",
    "native": "npm-run-all -s native:prep native:copynode native:postject",
    "test": "jest"
  },
  "engines": {
    "node": ">=24.0.0 <25",
    "npm": "please-use-yarn",
    "yarn": ">= 1.17.3"
  },
  "binary": {
    "napi_versions": [
      8
    ]
  },
  "bin": "dist/main.js",
  "devDependencies": {
    "@nestjs/cli": "^10",
    "@nestjs/testing": "^10",
    "@types/bindings": "^1.5.5",
    "@types/cli-color": "^2.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^7.7.2",
    "@typescript-eslint/parser": "^7.7.2",
    "@types/yargs": "^17.0.35",
    "bindings": "^1.5.0",
    "cmake-js": "^7.1.0",
    "coveralls": "^3.1.1",
    "esbuild": "^0.27.0",
    "eslint": "^8.57.0",
    "eslint-find-rules": "^4.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "jest": "^30.2.0",
    "node-addon-api": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "postject": "^1.0.0-alpha.6",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "zod2md": "^0.2.4"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "cli-color": "^2.0.4",
    "prompts": "^2.4.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "7.1.0",
    "telegraf": "^4.16.3",
    "yargs": "^18.0.0",
    "zod": "^4.1.12"
  }
}
